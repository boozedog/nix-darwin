{
  "formatting": {
    "command": ["nixfmt"]
  },
  "diagnostic": {
    "suppress": [-32001]
  },
  "options": {
    "nixpkgs": {
      "expr": "import (builtins.getFlake (toString ./.)).inputs.nixpkgs { }"
    },
    "darwin": {
      "expr": "(builtins.getFlake (toString ./.)).darwinConfigurations.mbp-m3-pro.options"
    },
    "home-manager": {
      "expr": "(builtins.getFlake (toString ./.)).homeConfigurations.david.options"
    }
  }
}